{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "request": "launch",
        "name": "Next.js: Debug App",
        "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
        "args": ["dev"],
        "cwd": "${workspaceFolder}",
        "env": {
          "PORT": "3001",
          "NODE_ENV": "development"
        },
        "console": "integratedTerminal",
        "outputCapture": "std"
      }
    ]
  }